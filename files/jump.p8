pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
--move with arrows jump with 0
--get to the flag
function _init()
    px=8-- x-position 
    py=8*13-- y-position 
    pstate=0-- current player state 
    pspr=0-- current sprite 
    pdir=0-- current direction
    pat=0-- player state timer
    win=0--win state
end 
function canfall()
    -- get the map tile under the player
    v=mget(flr((px+4)/8),flr((py+8)/8))
    -- see if it's flagged as wall
    return not fget(v,0) 
end
function iswon()
    -- get the map tile under the player
    v=mget(flr((px+4)/8),flr((py)/8))
    -- see if it's flagged as wall
    return fget(v,1) 
end
function change_state(s)
    pstate=s
    pat=0
end
function _draw()   
    -- draw the world 
    map(0,0,0,0,16,16)   
    -- draw the player, we use dir to mirror sprites          
    spr(pspr,px,py,1,1,pdir==-1)
    if win == 1 then
        print("jump person prevails!",8*5+2,8*8,rnd(11))
    end
end
function _update()
    if iswon() and win == 0 then
        sfx(2)
        win = 1
    elseif win == 0 then
        b0=btn(0)-- button0 state
        b1=btn(1)-- button1 state
        b2=btn(4)-- button2 state
        px=(px+128)%128-- no bounds left and right
        pat+=1-- increment state clock
        -- idle state 
        if pstate==0 then
            pspr=0
            if (b0 or b1) change_state(1)
            if (b2)then
                change_state(3)
                sfx(0)
            end
            if (canfall()) change_state(2)
        end
        -- walk state 
        if pstate==1 then
            if (b0)pdir=-1
            if (b1) pdir=1
            px+=pdir*min(pat,2)
            pspr=flr(pat/2)%2
            if (not (b0 or b1)) change_state(0)
            if (b2)then
                change_state(3)
                sfx(0)
            end
            if (canfall()) change_state(2)
        end
        -- fall state
        if pstate==2 then
            pspr=2
            if (canfall()) then
                if (b0) px-=1-- steer left
                if (b1) px+=1 -- steer right
                py+=min(4,pat)-- move the player
                if (not canfall())py=flr(py/8)*8-- check ground contact
            else
                py=flr(py/8)*8
                -- fix position when we hit ground
                change_state(0)
            end
        end
        -- jump state
        if pstate==3 then
            pspr=2
            py-=6-pat
            if (b0) px-=2
            if (b1) px+=2
            if (not b2 or pat>7) change_state(0)
        end
        --out of bounds
        if (py > 140) then
            px=8
            py=8*13
            sfx(01)
        end
    end
end
