pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
--draws colourful circles
frame = 0
textframe = 0
textx = rnd(100)
texty = rnd(100)
function _init()
	cls()
end
function _update() 
	frame += 1 
	textframe +=1
	if textframe > 60 then
		textframe = 0
		textx = rnd(100)
		texty = rnd(100)
	end
	-- loop at 15000
	if frame > 15000 then
		frame = 0
	end
	i = frame
	ccounter = 0
	while i > 0 do
		if i < 100 then
			color = ccounter
			circfill(64,64, i, color)
		end
		i -= 1
		ccounter += 1
	end
	print("o", textx+4,texty,rnd(15))
	print("b", textx+8,texty,rnd(15))
	print("e", textx+12,texty,rnd(15))
	print("y", textx+16,texty,rnd(15))
end
--[[function _draw()
	i = 1 
	while( i < frame) do   
		circ(64,64, i, 15)
		i += 1 
	end 
end]]

